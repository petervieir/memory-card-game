{
  "name": "memory-card-game",
  "version": "1.0.0",
  "description": "Memory Card Game on Stacks with Next.js frontend and Clarity smart contracts",
  "private": true,
  "scripts": {
    "// Development": "",
    "dev:frontend": "cd front-end && npm run dev",
    "dev:contracts": "cd clarity && npm run console",
    "// Building": "",
    "build:frontend": "cd front-end && npm run build",
    "build:contracts": "cd clarity && npm run check",
    "// Testing": "",
    "test:frontend": "cd front-end && npm run test",
    "test:contracts": "cd clarity && npm run test",
    "// Code Quality": "",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "// Deployment": "",
    "deploy:devnet": "cd clarity && npm run deploy:devnet",
    "deploy:testnet": "cd clarity && npm run deploy:testnet",
    "// Utilities": "",
    "clean:all": "rm -rf node_modules && rm -rf front-end/node_modules && rm -rf clarity/node_modules",
    "setup": "cd front-end && npm install && cd ../clarity && npm install",
    "start:frontend": "cd front-end && npm run start"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "npm@10.0.0",
  "author": "",
  "license": "MIT"
}
